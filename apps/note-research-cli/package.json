{
  "name": "note-research-cli",
  "version": "0.2.0",
  "type": "module",
  "bin": {
    "note-research": "dist/main.js"
  },
  "main": "dist/main.js",
  "exports": {
    ".": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "bundleDependencies": [
    "@note-research/note-core"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run typecheck",
    "test": "npm run build && node --test tests/*.test.js",
    "start": "node dist/main.js",
    "bundle:note-core": "node scripts/bundle-note-core.cjs"
  },
  "dependencies": {
    "@note-research/note-core": "file:../../packages/note-core",
    "commander": "^12.1.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2"
  },
  "optionalDependencies": {
    "playwright": "^1.58.2"
  }
}
